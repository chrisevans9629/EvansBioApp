<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EvansBioApp.Views.MainPage"
             Title="{Binding Title}">

    <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
        <Button Text="About" Command="{Binding AboutPageCommand}"></Button>
        <Button Text="Skills" Command="{Binding SkillsPageCommand}"></Button>
        <Button Text="Contact Me" Command="{Binding ContactMePageCommand}"></Button>
    </StackLayout>
</ContentPage>